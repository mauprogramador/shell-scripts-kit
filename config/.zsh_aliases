alias drma="docker rm -f $(docker ps -a -q)"
alias dlsa="docker ps --format 'table {{.ID}}\t{{.Image}}\t{{.Names}}'"
alias sailup="./vendor/bin/sail up -d --build"
alias av="source .venv/bin/activate"
alias dv="deactivate"
alias reqs="pip3 freeze > requirements.txt"
alias zshrc="gedit ~/.zshrc"
alias zshalias="gedit ~/.zsh_aliases"
alias spaceshiprc="gedit ~/.spaceshiprc.zsh"
alias gitignore="cp ~/Templates/.gitignore .gitignore"
alias mine="java -jar ~/Games/TLauncher.v10/TLauncher.jar && exit"
alias changelog="bash ~/Coding/Shell/generate_changelogs.sh"
alias venv="bash ~/Coding/Shell/setup.sh"
alias setup="cp ~/Coding/Shell/setup.sh setup.sh"
alias precommit="cp ~/Templates/pre-commit .git/hooks/pre-commit"
alias release="bash ~/Coding/Shell/generate_releases.sh"
alias ptag="bash ~/Coding/Shell/push_new_tag.sh"
alias cb="xclip -selection clipboard"
